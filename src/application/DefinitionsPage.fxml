<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DefinitionsViewController">
  <children>
    <AnchorPane maxHeight="400.0" maxWidth="600.0" prefHeight="400.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
      <children>
            <VBox prefHeight="400.0" prefWidth="96.0" style="-fx-background-color: gray;">
               <children>
                  <Label fx:id="welcomeLabel" alignment="TOP_LEFT" contentDisplay="CENTER" prefHeight="17.0" prefWidth="64.0" text="Welcome," />
                  <Label fx:id="userLabel" text="User" />
                  <Button fx:id="effortConsoleButton" mnemonicParsing="false" onAction="#toEffortConsole" prefHeight="25.0" prefWidth="114.0" text="EffortConsole" />
                  <Button fx:id="defectConsoleButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="106.0" text="DefectConsole" />
                  <Button fx:id="planningPokerButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="106.0" text="PlanningPoker" />
                  <Button fx:id="viewLogsButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="113.0" text="ViewLogs" />
                  <Button fx:id="definitionsButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="113.0" text="Definitions" />
               </children>
            </VBox>
            <Label layoutX="300.0" layoutY="7.0" text="Definitions">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            
            <Label layoutX="195.0" layoutY="35.0" text="Projects" />
            <TextField fx:id="projectField" layoutX="146.0" layoutY="52.0" prefHeight="25.0" prefWidth="100.0" />
            <Button fx:id="addProjectButton" layoutX="252.0" layoutY="52.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="40.0" text="Add" />
            <ComboBox fx:id="projectDropDown" layoutX="146.0" layoutY="82.0" prefHeight="25.0" prefWidth="150.0" />
            
            <Label layoutX="416.0" layoutY="35.0" text="Life Cycle Steps" />
            <TextField fx:id="lifeCycleField" layoutX="385.0" layoutY="53.0" prefHeight="25.0" prefWidth="107.0" />
            <Button fx:id="addLifecycleButton" layoutX="492.0" layoutY="54.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="40.0" text="Add" />
            <ComboBox fx:id="lifeCycleDropDown" layoutX="385.0" layoutY="82.0" prefWidth="150.0" />
            
            <Label layoutX="161.0" layoutY="117.0" text="Effort Categories" />
            <TextField fx:id="effortCategoryField" layoutX="146.0" layoutY="135.0" prefHeight="25.0" prefWidth="100.0" />
            <Button fx:id="addEffortCategoryButton" layoutX="251.0" layoutY="135.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="40.0" text="Add" />
            <ComboBox fx:id="effortCategoryDropDown" layoutX="145.0" layoutY="164.0" prefHeight="25.0" prefWidth="150.0" />
            
            <Label layoutX="431.0" layoutY="117.0" text="Plans" />
            <TextField fx:id="planField" layoutX="385.0" layoutY="138.0" prefHeight="25.0" prefWidth="107.0" />
            <Button fx:id="addPlanButton" layoutX="494.0" layoutY="138.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="40.0" text="Add" />
            <ComboBox fx:id="planDropDown" layoutX="386.0" layoutY="167.0" prefHeight="25.0" prefWidth="150.0" />
            
            <Label layoutX="161.0" layoutY="203.0" text="Deliverables" />
            <TextField fx:id="deliverablesField" layoutX="146.0" layoutY="220.0" prefHeight="25.0" prefWidth="100.0" />
            <Button fx:id="addDeliverablesButton" layoutX="252.0" layoutY="220.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="40.0" text="Add" />
            <ComboBox fx:id="deliverablesDropDown" layoutX="147.0" layoutY="249.0" prefHeight="25.0" prefWidth="150.0" />
            
            <Label layoutX="427.0" layoutY="202.0" text="Interruptions" />
            <TextField fx:id="interruptionsFiled" layoutX="389.0" layoutY="218.0" prefHeight="25.0" prefWidth="100.0" />
            <Button fx:id="addInterruptionsButton" layoutX="492.0" layoutY="218.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="40.0" text="Add" />
            <ComboBox fx:id="interruptionsDropDown" layoutX="387.0" layoutY="247.0" prefHeight="25.0" prefWidth="150.0" />
            
            <Label layoutX="283.0" layoutY="289.0" text="Defect Categories" />
            <TextField fx:id="defectCategoryField" layoutX="270.0" layoutY="306.0" prefHeight="25.0" prefWidth="100.0" />
            <Button fx:id="addDefectCategoryButton" layoutX="375.0" layoutY="306.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="40.0" text="Add" />
            <ComboBox fx:id="defectCategoryDropDown" layoutX="269.0" layoutY="335.0" prefHeight="25.0" prefWidth="150.0" />
      
      </children>
    </AnchorPane>
  </children>
</VBox>
